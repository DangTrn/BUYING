;, type text}, &#13;&#10;            {&quot;DIV&quot;, type text}, &#13;&#10;            {&quot;CAT&quot;, type text}, &#13;&#10;            {&quot;AGE&quot;, type text}, &#13;&#10;            {&quot;SUB_SILHOUETE&quot;, type text}, &#13;&#10;            {&quot;Store Cd&quot;, type text}, &#13;&#10;            {&quot;SHORT NAME&quot;, type text}, &#13;&#10;            {&quot;UPS_STANDARD&quot;, Int64.Type}, &#13;&#10;            {&quot;QTY_BY_SKU&quot;, Int64.Type}})&#13;&#10;in&#13;&#10;    #&quot;Changed Type&quot;]]&gt;&lt;/Formula&gt;&lt;IsParameterQuery xsi:nil=&quot;true&quot; /&gt;&lt;IsDirectQuery xsi:nil=&quot;true&quot; /&gt;&lt;/Query&gt;&lt;Query Name=&quot;MASTER_FILE&quot;&gt;&lt;Formula&gt;&lt;![CDATA[let&#13;&#10;    Source = Sql.Database(&#13;&#10;        &quot;ACFC-L-DANGNIKE\SQLEXPRESS&quot;, &#13;&#10;        &quot;NIKE&quot;, &#13;&#10;        [Query=&quot;&#13;&#10;            select *, &#13;&#10;            ROUND(SRP/300000,0)*0.3 GR_SRP&#13;&#10;            from MASTER_FILE&#13;&#10;            WHERE VEND='NIKE'&quot;])&#13;&#10;in&#13;&#10;    Source]]&gt;&lt;/Formula&gt;&lt;IsParameterQuery xsi:nil=&quot;true&quot; /&gt;&lt;IsDirectQuery xsi:nil=&quot;true&quot; /&gt;&lt;/Query&gt;&lt;Query Name=&quot;DOOR_PROFILE&quot;&gt;&lt;Formula&gt;&lt;![CDATA[let&#13;&#10;    Source = Sql.Database(&#13;&#10;        &quot;ACFC-L-DANGNIKE\SQLEXPRESS&quot;, &#13;&#10;        &quot;NIKE&quot;, &#13;&#10;        [Query=&quot;&#13;&#10;            SELECT &#13;&#10;    &#9;        [Store Cd], &#13;&#10;&#9;        CASE&#13;&#10;&#9;&#9;    WHEN LEVEL= 'NSP Best' then 'NSPBST'&#13;&#10;&#9;&#9;    WHEN LEVEL= 'NSP Better 1' THEN 'NSPBTR1'&#13;&#10;&#9;&#9;    WHEN LEVEL= 'NSP Better 2' THEN 'NSPBTR2'&#13;&#10;&#9;&#9;    WHEN LEVEL= 'DS' THEN 'DS'&#13;&#10;&#9;&#9;    ELSE 'ALC'&#13;&#10;&#9;        END AS LEVEL,&#13;&#10;                AREA, &#13;&#10;&#9;        [SHORT NAME], &#13;&#10;                LEFT(CAT_DIV,3) CAT, &#13;&#10;                RIGHT(C